{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
      "color": {
      "defaultPalette": false,
      "palette": [
        {
          "name": "Page - Background",
          "color": "#fafafa",
          "slug": "page-bg"
        },
        {
          "name": "Page - Text",
          "color": "#222",
          "slug": "page-text"
        },
        {
          "name": "Page - Text - Downplayed",
          "color": "#52525b",
          "slug": "page-text-downplayed"
        },
        {
          "name": "Page - Border",
          "color": "#d4d4d8",
          "slug": "page-border"
        },
        {
          "name": "Page - Border - Highlight",
          "color": "#a1a1aa",
          "slug": "page-border-highlight"
        },
        {
          "name": "Splash Page - Background",
          "color": "#f4f4f5",
          "slug": "splash-page-bg"
        },
        {
          "name": "Splash Page - Text",
          "color": "#222",
          "slug": "splash-page-text"
        },
        {
          "name": "Header - Background",
          "color": "#222",
          "slug": "header-bg"
        },
        {
          "name": "Header - Text",
          "color": "#fafafa",
          "slug": "header-text"
        },
        {
          "name": "Menu - Background",
          "color": "#fafafa",
          "slug": "menu-bg"
        },
        {
          "name": "Menu - Background - Mobile",
          "color": "#222",
          "slug": "menu-bg-mobile"
        },
        {
          "name": "Menu - Background - Artist",
          "color": "#222",
          "slug": "menu-bg-artist"
        },
        {
          "name": "Menu Text",
          "color": "#222",
          "slug": "menu-text"
        },
        {
          "name": "Language Switcher - Background",
          "color": "#f4f4f5",
          "slug": "language-switcher-bg"
        },
        {
          "name": "Language Switcher - Border",
          "color": "#222",
          "slug": "language-switcher-border"
        },
        {
          "name": "Language Switcher - Background - Highlight",
          "color": "#d4d4d8",
          "slug": "language-switcher-bg-highlight"
        },
        {
          "name": "Button - Background ",
          "color": "#222",
          "slug": "button-bg"
        },
        {
          "name": "Button - Text",
          "color": "#fafafa",
          "slug": "button-text"
        },
        {
          "name": "Button - Background - Highlight",
          "color": "#000",
          "slug": "button-bg-highlight"
        },
        {
          "name": "Footer - Background",
          "color": "#09090b",
          "slug": "footer-bg"
        },
        {
          "name": "Spotify",
          "color": "#1ED760",
          "slug": "spotify"
        },
        {
          "name": "Dark Grey",
          "color": "#222",
          "slug": "darkgrey"
        }
      ],
        "background": false,
        "link": false,
        "text": false
      },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Inter, ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
          "slug": "inter",
          "name": "Inter"
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "inter"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color-page-text)"
        },
        "typography": {
          "textDecoration": "none"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset-color-page-text)"
          }
        }
      },
      "button": {
        "color": {
          "text": "var(--wp--preset--color-page-text)",
          "background": "var(--wp--preset--color-page-bg)"
        },
        "typography": {
          "fontWeight": "700",
          "fontSize": "1rem"
        },
        "spacing": {
          "padding": {
            "top": "0.5rem",
            "bottom": "0.5rem",
            "right": "1rem",
            "left": "1rem"
          }
        }
      },
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--page-text)"
        },
        "typography": {
          "fontWeight": "700"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "1.802rem",
          "lineHeight": "2.116rem"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "1.602rem",
          "lineHeight": "1.931rem"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "1.424rem",
          "lineHeight": "1.794rem"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "1.266rem",
          "lineHeight": "1.486rem"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "1.125rem",
          "lineHeight": "1.321rem"
        }
      }
    },
    "blocks": {
    }
  }
}