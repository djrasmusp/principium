  "styles": {
    "typography": {
      "fontFamily": "inter"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color-primary-700)"
        },
        "typography": {
          "textDecoration": "none"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset-color-primary-900)"
          }
        }
      },
      "button": {
        "color": {
          "text": "var(--wp--preset--color--primary-900)",
          "background": "var(--wp--preset--color--primary-300)"
        },
        "typography": {
          "fontWeight": "700",
          "fontSize": "1rem"
        },
        "spacing": {
          "padding": {
            "top": "0.5rem",
            "bottom": "0.5rem",
            "right": "1rem",
            "left": "1rem"
          }
        }
      },
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--gray-900)"
        },
        "typography": {
          "fontWeight": "700"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "1.802rem",
          "lineHeight": "2.116rem"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "1.602rem",
          "lineHeight": "1.931rem"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "1.424rem",
          "lineHeight": "1.794rem"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "1.266rem",
          "lineHeight": "1.486rem"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "1.125rem",
          "lineHeight": "1.321rem"
        }
      }
    },
